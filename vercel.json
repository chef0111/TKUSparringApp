{
  "buildCommand": null,
  "outputDirectory": ".",
  "devCommand": null,
  "installCommand": null,
  "framework": null,
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/app/$1"
    }
  ],
  "routes": [
    {
      "src": "/",
      "dest": "/app/splash.html"
    },
    {
      "src": "/index.html",
      "dest": "/app/index.html"
    },
    {
      "src": "/splash.html",
      "dest": "/app/splash.html"
    },
    {
      "src": "/transition.html",
      "dest": "/app/transition.html"
    }
  ],
  "headers": [
    {
      "source": "/src/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    }
  ]
}

